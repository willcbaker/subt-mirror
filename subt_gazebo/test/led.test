<launch>

  <param name="/use_sim_time" value="true" />

  <!-- gazebo server-->
  <node name="gazebo" pkg="gazebo_ros" type="gzserver" respawn="false" output="screen" args="-r $(find subt_gazebo)/test/led_test.world --verbose" />

  <test test-name="led" pkg="subt_gazebo" type="test_led" time-limit="15.0" />

</launch>
